<ResourceDictionary 
    x:Class="HobbyEditor.Dictionaries.ControlTemplates"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:HobbyEditor.Dictionaries">
    <Style TargetType="{x:Type TextBox}" x:Key="TextBoxStyle">
        
    </Style>

    <Style TargetType="{x:Type TextBox}" x:Key="{x:Type TextBox}" 
           BasedOn="{StaticResource TextBoxStyle}">
        <EventSetter Event="KeyDown" Handler="_onTextBoxKeyDown"/>
    </Style>
    
</ResourceDictionary>